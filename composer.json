{
    "name": "elliot-sawyer/bootswatcher",
    "type": "silverstripe-recipe",
    "description": "Silverstripe project with Bootswatch themes preinstalled",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "~4.9.0@stable",
        "silverstripe/login-forms": "~4.5.0@stable",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "larapack/dd": "^1.1"
    },
    "extra": {
        "expose": [
            "themes/bootswatcher/dist/css"
        ],
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/app.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
